@model WebSQL.Areas.Admin.Controllers.UserCreateView
@{
    ViewBag.Title = "Edit User";
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="card-header">
            </div>
            <div class="body">
                <form action="@Url.Action("EditUser","Roles",new { @area="Admin" })" method="post" enctype="multipart/form-data">
                    <div class="form-group">
                        @{ Html.RenderPartial("_CreateOrEdit", Model);}
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
