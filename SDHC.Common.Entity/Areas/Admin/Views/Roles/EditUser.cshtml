@model WebSQL.Areas.Admin.Controllers.UserCreateView
@{
    ViewBag.Title = "Edit User";
    ViewBag.inEdit = true;
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="body">
                <form autocomplete="off" action="@Url.Action("EditUser","Roles",new { @area=E.AdminPath })" method="post" enctype="multipart/form-data">
                    @{ Html.RenderPartial("_CreateOrEdit", Model);}
                </form>
            </div>
        </div>
    </div>
</div>
