@using SDHC.Common.Entity.Models
@model ContentPostModel
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout_Content.cshtml";
}
@section breadcrumb{
    @{ Html.RenderPartial("_BreadCrumbContent", Model.ConvertToBaseModel());}
}
<div class="card">
    <div class="card-block">
        <h4>Default Basic Forms</h4>
        <form action="@Url.Action("Create","Content",new { @area="Admin" })" class="form-horizontal m-t-40" method="post" enctype="multipart/form-data">
            @{ Html.RenderPartial("_CreateOrEdit", Model);}
        </form>

    </div>

</div>
