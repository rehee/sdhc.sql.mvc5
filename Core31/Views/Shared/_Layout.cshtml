      <script src="~/admin-lib/plugins/boootstrap-datepicker/bootstrap-datepicker.min.js"></script>
      <script src="~/admin-lib/plugins/bootstrap-datetime-picker/js/bootstrap-datetimepicker.js"></script>
      <script src="~/admin-lib/plugins/bootstrap-timepicker/js/bootstrap-timepicker.js"></script>
      <script src="~/admin-lib/plugins/bootstrap-daterange/daterangepicker.js"></script>
      <script src="~/admin-lib/plugins/ckeditor/ckeditor.js"></script>
      <script src="~/admin-lib/plugins/jquery-spinner/js/jquery.spinner.js"></script>
