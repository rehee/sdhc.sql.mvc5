@model UserCreateView
@{
    ViewBag.Title = "Create User";
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="body">
                <form action="@Url.Action("CreateUser","Roles",new { @area = "Admin" })" method="post" enctype="multipart/form-data">
                    @{ await Html.RenderPartialAsync("_CreateOrEdit", Model);}
                </form>
            </div>
        </div>
    </div>
</div>
