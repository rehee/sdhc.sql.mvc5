@Html.Hidden("PostBeforeMethod", Model.PostBeforeMethod, new { @id = "PostBeforeMethod" + randomKey })
@Html.Hidden("PostResponseMethod", Model.PostResponseMethod, new { @id = "PostResponseMethod" + randomKey })
